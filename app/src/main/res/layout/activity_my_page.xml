<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyPage">

    <RelativeLayout
        android:id="@+id/test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/iv_title"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:src="@drawable/pont" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/bt_myPage_back"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="13dp"
            android:background="@drawable/back_press"
            android:backgroundTint="#716C6C"
            android:textSize="20sp"
            android:textStyle="bold" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="60dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test">
        <TextView
            android:id="@+id/tv_login_login"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:text="마이페이지"
            android:background="@drawable/line_bottom"
            android:layout_marginBottom="8dp"
            android:textAlignment="center"
            android:textSize="26sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_berry" />


    </LinearLayout>

    <Button
        android:id="@+id/bt_myPage_edit"
        android:layout_width="0dp"
        android:layout_height="77dp"
        android:layout_marginTop="124dp"
        android:backgroundTint="#EB7777"
        android:text="개인정보 수정"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test" />

    <Button
        android:id="@+id/button8"
        android:layout_width="0dp"
        android:layout_height="77dp"
        android:layout_marginTop="200dp"
        android:backgroundTint="#F6BFD2"
        android:text="등록 경매 확인"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test" />

    <Button
        android:id="@+id/bt_myPage_commentCheck"
        android:layout_width="0dp"
        android:layout_height="77dp"
        android:layout_marginTop="276dp"
        android:backgroundTint="#EB7777"
        android:text="경매 참여 목록"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test" />

    <Button
        android:id="@+id/button11"
        android:layout_width="0dp"
        android:layout_height="77dp"
        android:layout_marginTop="352dp"
        android:backgroundTint="#F6BFD2"
        android:text="관심 상품 확인"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test" />

    <Button
        android:id="@+id/bt_myPage_interCheck"
        android:layout_width="0dp"
        android:layout_height="77dp"
        android:layout_marginTop="352dp"
        android:backgroundTint="#EB7777"
        android:text="내 스크랩 목록"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test" />

    <Button
        android:id="@+id/bt_myPage_auctionCheck"
        android:layout_width="0dp"
        android:layout_height="77dp"
        android:layout_marginTop="200dp"
        android:backgroundTint="#EB7777"
        android:text="내 경매 목록"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/lo_auction_layout"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/line_top"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bt_auction"
                android:layout_width="69dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/category_press" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bt_chat"
                android:layout_width="69dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/chat2_press" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bt_home"
                android:layout_width="69dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/home_press" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bt_scrap"
                android:layout_width="69dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/scrap_press" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bt_myPage"
                android:layout_width="69dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/mypage2_press" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="경매"
                android:textSize="17dp"
                android:textAlignment="center"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="채팅"
                android:textAlignment="center"
                android:textSize="17dp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="홈"
                android:textAlignment="center"
                android:textSize="17dp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="스크랩"
                android:textAlignment="center"
                android:textSize="17dp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/textView"
                android:layout_width="35dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="내정보"
                android:textAlignment="center"
                android:textSize="17dp"
                android:textStyle="bold"/>
        </LinearLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>